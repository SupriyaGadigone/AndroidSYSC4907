<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.supriyagadigone.androidsysc4907"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.NFC" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-feature android:name="android.hardware.nfc" android:required="true"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReadNfc">
            <intent-filter>
                <action android:name="android.nfc.action.TECH_DISCOVERED" />
            </intent-filter>

            <meta-data
                android:name="android.nfc.action.TECH_DISCOVERED"
                android:resource="@xml/nfc_tech_filter" />
        </activity>
        <activity android:name=".Customer.Quiz.QuizActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".Organization.OrgWriteNfc"></activity>
        <activity android:name=".Organization.OrgEditProduct"></activity>
        <activity android:name=".Customer.CustomerGroceryListPage"></activity>
        <activity android:name=".Customer.CustomerProfilePage"></activity>
        <activity android:name=".Customer.CustomerReadNfcData"></activity>
        <activity android:name=".Customer.CustomerGroceryList"></activity>
        <activity android:name=".Customer.CustomerTappedProducts"></activity>
        <activity android:name=".Customer.CustomerTappedProductInfo"></activity>
        <activity android:name=".Organization.OrgAllProducts"></activity>
    </application>

</manifest>